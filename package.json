{
  "name": "@turbodocx/next-plugin-llms",
  "version": "0.1.0",
  "description": "Next.js plugin for generating LLM-friendly documentation following the llmstxt.org standard",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "next-plugin",
    "llm",
    "llms",
    "llmstxt",
    "documentation",
    "ai",
    "automation"
  ],
  "author": "TurboDocx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/turbodocx/next-plugin-llms.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.26.4",
    "gray-matter": "^4.0.3",
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "memfs": "^4.14.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
